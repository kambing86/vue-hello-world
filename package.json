    "eslint-plugin-vue": "^7.17.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.0",
    "sass": "^1.41.0",
    "sass-loader": "^10.2.0",
    "typescript": "~4.4.3",
    "vue-cli-plugin-vuetify": "~2.4.2",
